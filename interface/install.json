[
  {".": "row", "title": "StarterUI Installation",
   "+": [{"$src": "/interface/navigation.json"}]},

  {"$": "horizontal-rule"},
  
  {".": "row", "+": [
    {".": "twelve columns", "title": "Index HTML", "description": "StarterUI is just one HTML page away from revolutionizing your development. Copy this index.html file into your project:",
     "+": [{"$": "code", "src": "/index.html"}, {"$": "space"}]
    }
  ]},
  {".": "row", "+": [
    {".": "six columns", "title": "Interface JSON", "description": "Place the following JSON under /interface/index.json:",
     "+": [{"$": "code", "src": "/interface/example.json"}]
    },
    {
      "$": "form", ".": "six columns", "title": "Rendered UI", "description": "Load your project index, it should now look like:",
      "+": [
        {"$": "fieldset", "title": "Rendered UI",
            "+": [{"$src": "/interface/example.json"}]
        }
      ]
    }
  ]},
  
  {"$": "space"}
]